.navbar-inverse, .navbar-nav{
background: #529f35;
background-image: url('feet.png');
height: 60px;
border: #529f35;
}

.navbar-inverse .navbar-nav > li > a {
color: #fff;
}




.navbar-inverse .navbar-brand{
	font-family: Oleo Script Swash Caps, serif;
	font-size: 1.5em;
	letter-spacing: .05em;
	color: #fff;
}

.pagination{
	display: block;
	padding: 8px;
	text-align: center;
	border: 1px dotted #237f8f;
	border-left: 0;
	border-right: 0;
	width: 20%;
	overflow: visible;
	margin: 0 auto;
}

.walk-box{

	 -moz-box-shadow: 1px 1px 2px 0 #d0d0d0;
  -webkit-box-shadow: 1px 1px 2px 0 #d0d0d0;
  box-shadow: 1px 1px 2px 0 #d0d0d0;
  background: #fff;
  border: 1px solid #ccc;
  border-color: #e4e4e4 #bebebd #bebebd #e4e4e4;
  margin-bottom: .75em;
 
}