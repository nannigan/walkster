* {
  border-radius: 0 !important;
  -moz-border-radius: 0 !important;
}

html{
	font-size: 16px;
	border-radius: 0;
}

body{
	font-size: 100%;
	font-family: Merriweather, serif;
	font-weight: 400;
	background-image: url('sunny.png');
}

h1, h2, h3, h4, h5, h6{
 color: #529f35;
 font-family: Oswald, sans-serif;
 font-weight: 700;
 letter-spacing: .07em;
}
a{
	color: #529f35;
}
