<div class='col-xs-10 col-xs-offset-1 walk-box'>
<h2>Share A Walk!</h2>
<%= simple_form_for @place do |f| %>
   <p> <%= f.input :name %></p>
    <p><%= f.input :address %></p>
    <p><%= f.input :description %></p>
    <button class='btn btn-primary-custom pull-right'><%= link_to "Cancel", root_path %></button>
    <%= f.submit 'Create', :class => 'btn btn-primary-custom' %> 
  <% end %>

</div>